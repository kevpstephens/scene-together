{
  "expo": {
    "name": "SceneTogether",
    "slug": "scenetogether",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "scenetogether",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app needs access to your calendar to add events you RSVP to.",
        "NSRemindersUsageDescription": "This app needs access to your reminders to set event reminders.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.kevpstephenson.scenetogether"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "aab8b84d-014a-47bb-97cd-ee6517680953"
      }
    }
  }
}
